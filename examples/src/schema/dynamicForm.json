{
  "type": {
    "type": "string",
    "label": "type",
    "format": "select",
    "options": [
      "mammal",
      "fish",
      "bird",
      "insect"
    ],
    "defaultValue": "fish"
  },
  "habitat": {
    "type": "object",
    "label": "habitat",
    "format": "form",
    "conditionalSchema": {
      "ref": "type",
      "switch": [
        {
          "condition": "fish",
          "schema": {
            "sea": {
              "type": "string",
              "label": "ocean or sea"
            }
          },
          "flow": ["sea"]
        },
        {
          "default": true,
          "schema": {
            "continent": {
              "type": "string",
              "label": "continent",
              "format": "select",
              "options": [
                "Africa",
                "Asia",
                "Europe",
                "North America",
                "Oc√©ania",
                "South America"
              ]
            },
            "country": {
              "type": "string",
              "label": "country"
            }
          },
          "flow": ["continent", "country"]
        }
      ]
    }
  }
}