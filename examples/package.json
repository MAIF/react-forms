{
  "name": "@maif/react-forms-playground",
  "version": "1.0.1",
  "private": true,
  "homepage": "https://maif.github.io/react-forms/",
  "dependencies": {
    "@maif/react-forms": "^1.6.9",
    "babel-standalone": "^6.26.0",
    "bootstrap": "5.1.3",
    "lz-string": "^1.4.4",
    "react-scripts": "^5.0.1",
    "react-select": "5.1.0",
